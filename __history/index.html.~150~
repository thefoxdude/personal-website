<script>
	var acc = document.getElementsByClassName("Accordion");
	var panels = document.getElementsByClassName("Panel");
	var i;
	var lastClicked = "";

	for (i = 0; i < acc.length; i++) {
		acc[i].onclick = function(){
			var currentClick = "";
			var boxID = this.id;
			var num = boxID.toString().slice(-1);
			var type = boxID.toString().charAt(0);
			currentClick = type + num;
			var boxToShow;
			if (type == "f") {
				boxToShow = document.getElementById("fpanel" + num);
			}
			else {
				boxToShow = document.getElementById("wpanel" + num);
			}
			accToggle(acc);
			panelToggle(panels);
        		
			if (currentClick == lastClicked) {
				lastClicked = "";
			}
			else {
				this.classList.toggle(type + "Active" + num);
				boxToShow.classList.toggle("show");
				lastClicked = currentClick;
			}
		}
	}
</script>

<script>
function openMenu() {
    var x = document.getElementById("links");
    if (x.className === "MyLinks") {
        x.className += " Responsive";
    } else {
        x.className = "MyLinks";
    }
}
</script>
	
<script>
	function accToggle(array) {
		var j;
		for (j = 0; j < array.length; j++) {
			array[j].className = "Image Accordion";
		}
	}
		
	function panelToggle(array) {
		var j;
		for (j = 0; j < array.length; j++) {
			array[j].className = "Panel";
		}
	}
</script>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script>
	$(function() {
  		$('a[href*="#"]:not([href="#"])').click(function() {
    		if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
      		var target = $(this.hash);
      		target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
      		if (target.length) {
        			$('html, body').animate({scrollTop: target.offset().top - 50}, 1000);
        			return false;
      		}
    		}
  		});
		});
</script>

<script>
	$(window).scroll(function () {
    	if ($(window).scrollTop() > 375) {
    		$('#topBar').attr("id", "newTopBar");
        	/*$('#topBar').css('top', $(window).scrollTop());*/
    	}
    	if ($(window).scrollTop() < 375) {
    		$('#newTopBar').attr("id", "topBar");
        	/*$('#topBar').css('top', $(window).scrollTop());*/
    	}
	}
	);
</script>

<script>
	var home = $("#introWhole").offset().top;
	var portfolio = $("#projectsWhole").offset().top;
	var aboutMe = $("#aboutMeWhole").offset().top;
	var resume = $("#resumeWhole").offset().top;
	var contact = $("#contactWhole").offset().top;

	var $w = $(window).scroll(function(){
		if ( $w.scrollTop() > home ) {   
			toggleSelected();
        	$('#home').find(">:first-child").attr("class", "Selected");
    	}
    	if ( $w.scrollTop() > portfolio - 100 ) {   
    		toggleSelected();
        	$('#portfolio').find(">:first-child").attr("class", "Selected");
    	}
    	if ( $w.scrollTop() > aboutMe - 100 ) {   
    		toggleSelected();
        	$('#aboutMeTop').find(">:first-child").attr("class", "Selected");
    	}
    	if ( $w.scrollTop() > resume - 100 ) {   
    		toggleSelected();
        	$('#resume').find(">:first-child").attr("class", "Selected");
    	}
    	if ( $w.scrollTop() > contact - 275 ) {   
    		toggleSelected();
        	$('#contact').find(">:first-child").attr("class", "Selected");
    	}
	});
	
	function toggleSelected() {
		document.getElementById("home").firstChild.className = "";
		document.getElementById("portfolio").firstChild.className = "";
		document.getElementById("aboutMeTop").firstChild.className = "";
		document.getElementById("resume").firstChild.className = "";
		document.getElementById("contact").firstChild.className = "";
	}
</script>