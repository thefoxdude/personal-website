	<script>
		function loadingText() {
			var randomNum = Math.floor((Math.random() * 9) +1);
			var text = document.getElementById("text" + randomNum).innerHTML;
			document.getElementById("textToChange").innerHTML = text;
			var signature = document.getElementById("signature" + randomNum).innerHTML;
			document.getElementById("signatureToChange").innerHTML = signature;
			
			var scrollLocation = document.body.scrollTop;
			if (scrollLocation > 375) {
				document.getElementById("topBar").id = 'newTopBar';
			}
		}
	</script>
</head>

