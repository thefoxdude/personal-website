<script>
	var acc = document.getElementsByClassName("Accordion");
	var panels = document.getElementsByClassName("Panel");
	var i;
	var lastClicked = "";

	for (i = 0; i < acc.length; i++) {
		acc[i].onclick = function(){
			var currentClick = "";
			var boxID = this.id;
			var num = boxID.toString().slice(-1);
			var type = boxID.toString().charAt(0);
			currentClick = type + num;
			var boxToShow;
			if (type == "f") {
				boxToShow = document.getElementById("fpanel" + num);
			}
			else {
				boxToShow = document.getElementById("epanel" + num);
			}
			accToggle(acc);
			panelToggle(panels);
        		
			if (currentClick == lastClicked) {
				lastClicked = "";
			}
			else {
				this.classList.toggle(type + "Active" + num);
				boxToShow.classList.toggle("show");
				lastClicked = currentClick;
			}    			
        		
	}
	}
</script>
	
<script>
	function accToggle(array) {
		var j;
		for (j = 0; j < array.length; j++) {
			array[j].className = "Image Accordion";
		}
	}
		
	function panelToggle(array) {
		var j;
		for (j = 0; j < array.length; j++) {
			array[j].className = "Panel";
		}
	}
</script>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script>
	$(function() {
  		$('a[href*="#"]:not([href="#"])').click(function() {
    		if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
      		var target = $(this.hash);
      		target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
      		if (target.length) {
        		$('html, body').animate({
          		scrollTop: target.offset().top
        		}, 1000);
        		return false;
      		}
    		}
  		});
		});
</script>

<script>
	$(window).scroll(function () {
    	if ($(window).scrollTop() > 375) {
    		$('#topBar').attr("id", "newTopBar");
        	/*$('#topBar').css('top', $(window).scrollTop());*/
    	}
    	if ($(window).scrollTop() < 375) {
    		$('#newTopBar').attr("id", "topBar");
        	/*$('#topBar').css('top', $(window).scrollTop());*/
    	}
	}
	);
</script>